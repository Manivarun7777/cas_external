    "def common_elements(lst1,lst2):\n",
    "    common=[]\n",
    "    for i in lst1:\n",
    "        if i  in lst2 and i not in common:\n",
    "            common.append(i)\n",
    "    return common\n",
    "lst1=[1,2,3,4]\n",
    "lst2=[3,4,5,6]\n",
    "result=common_elements(lst1,lst2)\n",
    "print(result)"
